<Window x:Class="ProductAccounting.Window2"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ProductAccounting"
        mc:Ignorable="d"
        Title="Window2" Height="400" Width="500"  WindowStartupLocation="CenterScreen">
    <Grid>
        <TextBox x:Name="Namewarehouse" BorderBrush="Black" FontSize="16" Width="300"  Height="20" HorizontalAlignment="Right" Margin="0,141,31,223"/>
        <TextBox x:Name="NamewarehouseText" IsReadOnly="True" FontSize="16"  Width="300" Height="20" HorizontalAlignment="Left" Margin="33,141,0,223">Склад</TextBox>
        <TextBox x:Name="Citywarehouse" BorderBrush="Black" FontSize="16" Width="200" Height="20" HorizontalAlignment="Left" Margin="269,82,0,82"/>
        <TextBox x:Name="CitywarehouseText" IsReadOnly="True" FontSize="16"  Width="200" Height="20" HorizontalAlignment="Left" Margin="33,182,0,182">Город</TextBox>
        <TextBox x:Name="Addresswarehouse" BorderBrush="Black" FontSize="16" Width="200" Height="20" HorizontalAlignment="Left" Margin="269,124,0,40"/>
        <TextBox x:Name="AddressWarehouseText" IsReadOnly="True" FontSize="16"  Width="200" Height="20" HorizontalAlignment="Left" Margin="33,224,0,140">Адрес</TextBox>
        <Button x:Name="OkButWarehouse" Content="Добавить" FontSize="16" Width="130" Height="30" VerticalAlignment="Top" Margin="175,306,175,0" Click="GetWarehouseInfo"/>
    </Grid>
</Window>
